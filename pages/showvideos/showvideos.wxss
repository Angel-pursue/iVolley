.container {
  display: flex;
  flex-direction: column;
  padding: 30rpx;
}

.video-list {
  display: flex;
  flex-direction: column;
}

.video-item {
  display: flex;
  align-items: center;
  margin-bottom: 40rpx;
}

.video-cover {
  width: 200rpx;
  height: 200rpx;
  border-radius: 10rpx;
  margin-right: 20rpx;
}

.video-title {
  font-size: 40rpx;
}
